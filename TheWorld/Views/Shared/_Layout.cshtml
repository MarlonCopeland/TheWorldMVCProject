@{
    ViewBag.Title = "Home Page Title";
}
<h1>The World</h1>
<!DOCTYPE html>
<html>
<head>
    <title>
        The World - @ViewBag.Title
    </title>
    <meta charset="utf-8" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet"/>
    <link href="~/lib/Font-Awesome/css/font-awesome.min.css"rel="stylesheet"/>
    <link href="~/css/site.css" rel="stylesheet" />

</head>
<body>
    <div id="sidebar">
        <img src="~/img/user1.jpg" alt="headshot" class="headshot" />
        <span id="userName">Marcus Lawson</span>
        <ul class="menu">
            <li class="active"><a asp-controller="App" asp-action="Index">Home</a></li>
            <li><a asp-controller="App" asp-action="About">About</a></li>
            <li><a asp-controller="App" asp-action="Contact">Contact</a></li>
        </ul>
    </div>
    <div id="wrapper">
        <div id="main">
            <div><button id="sidebarToggle" class="btn btn-primary">
                <i class="fa fa-angle-left"></i></button></div>
           @RenderBody()
        </div>
        <div id="footer">
            &copy; 2017 The World
        </div>
    </div>
    <script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    @RenderSection("scripts", false)  <!-- Renders scripts down here,even if they are at the top-->
    <script src="~/js/site.js"></script>
</body>
</html>